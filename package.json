{
  "name": "@wrtools/dojo-webpack-plugin",
  "version": "2.0.0",
  "author": "Chuck Dumont",
  "description": "Supports using webpack with Dojo 1.x applications",
  "scripts": {
    "lint": "eslint . --ext .json --ext .md --ext .js",
    "pretest": "npm run lint && node buildDojo/build.js node_modules/dojo/dojo.js test/js",
    "test": "mocha --harmony",
    "travis:test": "npm run -s test",
    "cover": "istanbul cover -x **/*.runtime.js node_modules/mocha/bin/_mocha",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "raw-loader": "0.5.1",
    "file-loader": "0.9.0",
    "tmp": "0.0.30"
  },
  "peerDependencies": {
    "webpack": ">= 2.2.0"
  },
  "license": "(MIT OR Apache-2.0)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/webratio/dojo-webpack-plugin.git"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "dijit": "1.12.2",
    "dojo": "1.12.2",
    "dojo-util": "1.12.2",
    "dojox": "1.12.2",
    "eslint": "^4.7.2",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "fs-extra": "^4.0.2",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.3",
    "mocha-lcov-reporter": "^1.3.0",
    "should": "^13.1.0",
    "webpack": "3.7.1"
  }
}
